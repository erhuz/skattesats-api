{
  "name": "skattetabeller-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erhuz/skattetabeller-api.git"
  },
  "keywords": [
    "tax-tables",
    "tax",
    "sweden",
    "skatteverket"
  ],
  "author": "Benjamin Fransson",
  "license": "MIT",
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "strict": [
        "error",
        "global"
      ],
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "warn"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/erhuz/skattetabeller-api/issues"
  },
  "homepage": "https://github.com/erhuz/skattetabeller-api#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "node-fetch": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^1.19.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
